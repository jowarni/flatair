<div id="page">
<h2>My booked flats</h2>
<div class="container">
  <div class="row">
      <% @flats.each do |flat|  %>
        <% flat.bookings.each do |b|  %>
          <div class="col-xs-12 col-sm-4 card">
            <h4><%= flat.title %></h4>
            <p><%= image_tag flat.picture.url(:medium) %></p>
            <p>From : <%= b.user.email %></p>
            <p>Arrival: <%= b.arrival %></p>
            <p>Departure: <%= b.departure %></p>
          </div>
        <% end %>
      <% end %>
  </div>
  <br>
</div>
</div>


