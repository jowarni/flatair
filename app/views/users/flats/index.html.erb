<div id="page">
<h2>My flats <%= link_to("+", new_flat_path, class: "btn btn-success btn-large text-center")%></h2>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-12">
      <% current_user.flats.each do |flat|  %>
      <div class="card">
        <h4><%=flat.title%></h4>
        <p>Price: <%=flat.price%>â‚¬</p>
        <p>City: <%=flat.city%></p>
        <p><%= image_tag flat.picture.url(:medium) %></p>
        <%= link_to("Delete", flat_path(flat), class: "btn btn-danger btn-xs btn-right", :method=>:delete)%><%= link_to("Edit", edit_flat_path(flat), class: "btn btn-success btn-xs btn-right mrg-right")%>
      </div>
      <%end%>
    </div>
  </div>
</div>
<div class="text-center">
</div>
